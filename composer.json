{
  "name": "roots/bedrock",
  "type": "project",
  "license": "MIT",
  "description": "A modern WordPress stack",
  "homepage": "https://roots.io/bedrock/",
  "authors": [
    {
      "name": "Scott Walkinshaw",
      "email": "scott.walkinshaw@gmail.com",
      "homepage": "https://github.com/swalkinshaw"
    },
    {
      "name": "Ben Word",
      "email": "ben@benword.com",
      "homepage": "https://github.com/retlehs"
    }
  ],
  "keywords": [
    "bedrock", "roots", "wordpress", "stack", "composer", "vagrant", "wp"
  ],
  "support": {
    "issues": "https://github.com/roots/bedrock/issues",
    "forum": "https://discourse.roots.io/category/bedrock"
  },
  "config": {
    "preferred-install": "dist",
    "secure-http":false
  },
  "repositories": [
    {
      "type": "composer",
      "url": "http://wpackagist.org"
    }
  ],
  "require": {
    "php": ">=5.5",
    "composer/installers": "~1.0.12",
    "vlucas/phpdotenv": "^2.0.1",
    "johnpbloch/wordpress": "4.4.1", 

    "wpackagist-plugin/custom-upload-dir": "3.4.3.1",       
    "wpackagist-plugin/disable-comments": "1.4",
    "wpackagist-plugin/duplicate-post": "2.6",
    "wpackagist-plugin/enable-media-replace": "3.0.4",
    "wpackagist-plugin/favicon-by-realfavicongenerator": "1.3.4",
    "wpackagist-plugin/image-widget": "4.2.2",    
    "roots/soil": "3.6.0",
    "wpackagist-plugin/siteorigin-panels": "2.4.4",    
    "wpackagist-plugin/svg-support": "2.2.2",    
    "wpackagist-plugin/store-locator-le": "4.4.37",
    "wpackagist-plugin/transients-manager": "1.7.2",    
    
  
    "wpackagist-plugin/woocommerce-ajax-add-to-cart-for-variable-products": "1.2.8",
    "wpackagist-plugin/woocommerce-cart-tab": "0.3.1",
    "wpackagist-plugin/load-more-products-for-woocommerce": "1.0.1",
    "wpackagist-plugin/woocommerce-multilingual": "3.7.14",
    "wpackagist-plugin/woocommerce-pdf-invoices-italian-add-on": "0.4.7",
    "wpackagist-plugin/woocommerce-pdf-invoices-packing-slips": "1.5.29",
    "wpackagist-plugin/woocommerce-remove-all-products": "1.0.4" 
    
    

  },  
  "require-dev": {
    "wpackagist-plugin/query-monitor": ">=2.8.1",    
    "wpackagist-plugin/debug-bar": ">=0.8.2"
    },
  "extra": {
    "installer-paths": {
      "web/app/mu-plugins/{$name}/": ["type:wordpress-muplugin"],
      "web/app/plugins/{$name}/": ["type:wordpress-plugin"],
      "web/app/themes/{$name}/": ["type:wordpress-theme"]
    },
    "wordpress-install-dir": "web/wp"
  }
}
